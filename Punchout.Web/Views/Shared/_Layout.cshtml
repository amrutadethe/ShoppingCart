<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    @*<title>@ViewBag.Title - ChemicoMays PunchOut</title>*@
    <title>ChemicoMays PunchOut</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    @*@Styles.Render("~/Styles/Site/css")*@

</head>
<body>
    <div id="outer">
        <div id="header">
            <div style="float: left;">
                <h1><a href="#">ChemicoMays PunchOut Catalog</a></h1>
            </div>
            <div style="float: right; vertical-align:middle; height: 103px;">
                <br /><br />
                <table cellpadding="0" cellspacing="0" border="0">
                    <tr style="vertical-align:middle;">
                        <td align="center" style="width:65px;"><a href="~/MyShoppingCart.aspx?method=noadd" runat="server" class="SiteLinkBold"><img src="~/Content/img/cart.gif" alt="" runat="server" /><br />Cart</a></td>
                    </tr>
                </table>
                <div class="cartContent" style="float: right;">

                </div>
            </div>
        </div>
        <div id="subheader">
            <div align="left">
                @Html.ActionLink("Home", "Index", "Home")
                &nbsp;&nbsp;
                @Html.ActionLink("New Item Additions", "", "HSEApproval")
                <h2>
                    Welcome Honeywell user.  Your site is @Session["Sitedesc"]
                    @*<% if (Session["SiteDesc"] == null)

                        Response.Write(" not selected.  Please select a site from the left.");
                        }
                        else
                        {
                        Response.Write(" set to " + Session["SiteDesc"] + ".");
                        } %>&nbsp;&nbsp;*@
                </h2>
            </div>
        </div>
        <div id="content">
            <div id="rightColumn">
            </div>
            <div id="mainContent">
                <div id="centerColumn">

                </div>
            </div>
            <div id="leftColumn">
                @{
                    Html.RenderPartial("PartialLayout", Model);
                }
                @*@RenderPage("~/Views/Home/PartialLayoutList.cshtml")*@
            </div>
            <div class="clear"></div>
        </div>
    </div>
    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>Copyright © 2020 ChemicoMays, LLC</p>
        </footer>
    </div>
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
